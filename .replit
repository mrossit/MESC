run = "NODE_ENV=development node node_modules/.bin/tsx server/index.ts"
modules = ["nodejs-20", "postgresql-16"]

[[ports]]
localPort = 5000
externalPort = 80

[nix]
channel = "stable-24_05"

[env]
NODE_ENV = "development"

[deployment]
deploymentTarget = "cloudrun"
run = ["sh", "-c", "npm start"]
build = ["sh", "-c", "npm run build"]
